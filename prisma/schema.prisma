generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id         String   @id @default(cuid())
  filename   String
  annotation String
  createdAt  DateTime @default(now())
}

model Job {
  id          String   @id @default(cuid())
  status      String   @default("pending")
  openaiJobId String?
  createdAt   DateTime @default(now())
}
