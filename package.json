{
  "name": "fine-tuning",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist **/.next **/.turbo **/database/**/generated *.bak && pnpm install",
    "compose": "docker compose -f ./docker/compose.dev.yaml --env-file .env",
    "dev": "pnpm compose up -d && pnpm run watch",
    "watch": "trap 'pnpm compose stop; exit 0' SIGINT; next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "ui": "pnpm dlx shadcn@latest",
    "start": "next start",
    "prisma": "prisma",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@prisma/adapter-pg": "6.14.0",
    "@prisma/client": "6.16.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "0.13.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.3.5",
    "openai": "^5.23.1",
    "postcss": "8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "2.5.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.17",
    "zod": "3.25.71"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "22.15.32",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "10.4.21",
    "dotenv": "17.2.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.5",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prisma": "6.16.2",
    "rimraf": "6.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.17.1"
}
